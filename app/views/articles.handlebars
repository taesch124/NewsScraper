{{#each articles}}
<div class="card">
    <div class="row">
        <div class="col s4">
            <img src="{{imageUrl}}">
        </div>
        <div class="col s8">
            <h3>{{title}}</h3>
            <p>{{summary}}</p>
            <a href="https://www.nytimes.com{{articleUrl}}" target="_blank">View article</a>
            <h3>{{author}}</h3>
        </div>
    </div>

</div>
{{/each}}