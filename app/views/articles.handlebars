<h2>Hello world</h2>
<p>Articles to come!</p>

{{#each articles}}
<div class="card">
    <div class="row">
        <div class="col s4">
            <img src="{{imageUrl}}">
        </div>
        <div class="col s8">
            <h3>{{title}}</h3>
            <p>{{summary}}</p>
            <h3>{{author}}</h3>
        </div>
    </div>

</div>
{{/each}}